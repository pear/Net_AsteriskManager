<refentry id="package.net.net-asteriskmanager.asteriskmanager.originatecall">
   <refnamediv>
    <refname><function>AsteriskManager::originateCall</function></refname>
    <refpurpose>Make a call to an extension with a given channel acting as the originator</refpurpose>
   </refnamediv>
   <refsynopsisdiv>
    <funcsynopsis>
     <funcsynopsisinfo>
      require_once &apos;/asterisk.php&apos;;
     </funcsynopsisinfo>
     <funcprototype>
      <funcdef>bool      <function>AsteriskManager::originateCall</function></funcdef>
      <paramdef>string <parameter>$extension</parameter></paramdef>
      <paramdef>string <parameter>$channel</parameter></paramdef>
      <paramdef>string <parameter>$context</parameter></paramdef>
      <paramdef>string <parameter>$cid</parameter></paramdef>
      <paramdef>integer <parameter> <optional>$priority = 1</optional></parameter></paramdef>
      <paramdef>integer <parameter> <optional>$timeout = 30000</optional></parameter></paramdef>
      <paramdef>array <parameter> <optional>$variables = &null;</optional></parameter></paramdef>
      <paramdef>string <parameter> <optional>$action_id = &null;</optional></parameter></paramdef>
     </funcprototype>
    </funcsynopsis>
    </refsynopsisdiv>

    <refsect1 id="package.net.net-asteriskmanager.asteriskmanager.originatecall.desc">
     &title.desc;
     &notdocumented;
    </refsect1>
   <refsect1 id="package.net.net-asteriskmanager.asteriskmanager.originatecall.param">
    &title.param;
    <para>
     <variablelist>
      <varlistentry>
       <term>
        
                        
                        &type.string;
                         <parameter>$extension</parameter>
       </term>
       <listitem>
        <para>
         The extension to use on connection
        </para>
       </listitem>
      </varlistentry>     
     </variablelist>
     <variablelist>
      <varlistentry>
       <term>
        
                        
                        &type.string;
                         <parameter>$channel</parameter>
       </term>
       <listitem>
        <para>
         The channel where you wish to originate the call
        </para>
       </listitem>
      </varlistentry>     
     </variablelist>
     <variablelist>
      <varlistentry>
       <term>
        
                        
                        &type.string;
                         <parameter>$context</parameter>
       </term>
       <listitem>
        <para>
         The context that the call will be dropped into
        </para>
       </listitem>
      </varlistentry>     
     </variablelist>
     <variablelist>
      <varlistentry>
       <term>
        
                        
                        &type.string;
                         <parameter>$cid</parameter>
       </term>
       <listitem>
        <para>
         The caller ID to use
        </para>
       </listitem>
      </varlistentry>     
     </variablelist>
     <variablelist>
      <varlistentry>
       <term>
        
                        
                        &type.int;
                         <parameter>$priority</parameter>
       </term>
       <listitem>
        <para>
         The priority of this command
        </para>
       </listitem>
      </varlistentry>     
     </variablelist>
     <variablelist>
      <varlistentry>
       <term>
        
                        
                        &type.int;
                         <parameter>$timeout</parameter>
       </term>
       <listitem>
        <para>
         Timeout in milliseconds before attempt dropped
        </para>
       </listitem>
      </varlistentry>     
     </variablelist>
     <variablelist>
      <varlistentry>
       <term>
        
                        
                        &type.array;
                         <parameter>$variables</parameter>
       </term>
       <listitem>
        <para>
         An array of variables to pass to Asterisk
        </para>
       </listitem>
      </varlistentry>     
     </variablelist>
     <variablelist>
      <varlistentry>
       <term>
        
                        
                        &type.string;
                         <parameter>$action_id</parameter>
       </term>
       <listitem>
        <para>
         A unique identifier for this command
        </para>
       </listitem>
      </varlistentry>     
     </variablelist>
    </para>
   </refsect1>
   <refsect1 id="package.net.net-asteriskmanager.asteriskmanager.originatecall.throws">
    &title.throws;
        <para>
      <emphasis>throws</emphasis> no exceptions thrown
    </para>
       </refsect1>
   <refsect1 id="package.net.net-asteriskmanager.asteriskmanager.originatecall.note">
    &title.note;
    &note.notstatic;
   </refsect1>
</refentry>
